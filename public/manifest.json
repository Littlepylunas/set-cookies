{
  "name": "Cookie Website additional",
  "description": "Add Cookie from file",
  "version": "1.0",
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "cookies",
    "<all_urls>",
    "tabs"
  ],
  "manifest_version": 2
}